<div class="panel-body">
    <form id="form-add">
        <div class="form-group">
            <label>Từ khóa</label>
            <input autocomplete="on" type="text" ng-model="LanguageModel.Key_Language" class="form-control" placeholder="NT_View_Key">
        </div>
        <div class="form-group">
            <label>Tiếng Việt</label>
            <input autocomplete="on" type="text" ng-model="LanguageModel.Language_VN" class="form-control" placeholder="Tiếng Anh">
        </div>
        <div class="form-group">
            <label>Tiếng Anh</label>
            <input autocomplete="on" type="text" ng-model="LanguageModel.Language_EN" class="form-control" placeholder="English">
        </div>
        <div class="d-flex justify-content-lg-start">
            <button ng-click="SubmitAdd()" type="button" class="btn mb-1 btn-success">
                Thêm<span class="btn-icon-right"><i class="fa fa-check"></i></span>
            </button>
            <button type="button" style="margin-left:10px;" class="btn mb-1 btn-info" ng-click="showPopupTable()">
                Trở về<span class="btn-icon-right"><i class="fa fa-arrow-left"></i></span>
            </button>
        </div>
    </form>
   
</div>

